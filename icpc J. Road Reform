#include "bits/stdc++.h"
using namespace std;
#define ll long long
#define fl i,a,b for(int i=a;i<b;i++)
#define co cout<<
#define ci cin>>
#define rep(i,x,n) for(int i = x; i < n; i++) 
#define rev(i,n,x) for(int i = n; i >= x; i--)
#define ff            first
#define ss            second
#define vecll vector<long long>
#define pii pair<long long ,long long >
#define pip pair<int,pair<int,int>>
#define ppp pair<pair<int,int>,pair<int,int>>
#define inarr(arr,x,n)  for(ll i=x ;i<n;i++) cin >> arr[i];
#define mpp make_pair
#define pb                push_back
#define ppb               pop_back
#define all(x)          (x).begin(),(x).end()
#define sz(x)           (x).size()
#define henshin           cout<<endl;
#define DEBUG(x) cout << (x) << '\n';
const int INF = int(1e9);
void yn(int x) { if(!x)cout << "NO\n";  else if(x)cout << "YES\n";return; }

const int N=2e5+5,M=1e6+5;
 
 const int MAXN = 2e5+7; 
      //vector<int> primes;
   // int mind[MAXA + 1];
// map<int,int> xprime;
 struct road{
    int x,y,s; 
 };

road* newroad(int a,int b,int c)
{
    road* temp = new road;
    temp->x = a;
    temp->y = b;temp->s = c;
    return temp;
}

void solve() {
ll n,m,k,b;ci n>>m>>k ;  
vector<road*> r(n);
 rep(i,0,m){ int x,y,s; ci x>>y>>s;r[i]= newroad(x,y,s); }

}
 

 
int main() {
#ifdef _DEBUG
    freopen("input.txt", "r", stdin);
    int tt = clock();
#endif
    ios_base::sync_with_stdio(false);
    cin.tie(0), cout.tie(0);
    cout << fixed << setprecision(15);
    
    int tc=1;    ci tc;  
/* 
  for (int i = 2; i <= MAXA; ++i) {
